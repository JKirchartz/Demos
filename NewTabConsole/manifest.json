{
  "manifest_version": 2,

  "name": "New Tab JS Repl",
  "description": "This replaces the New Tab page with a Javascript Repl",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "main.html"
  }
}
